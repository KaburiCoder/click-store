model Cart {
  id        Int        @id @default(autoincrement())
  ykiho     String
  cartItems CartItem[]
}

model CartItem {
  id          Int       @id @default(autoincrement())
  cartId      Int       @map("cart_id")
  code        String
  quantity    Int
  fit         Boolean
  createdDate DateTime  @default(now()) @map("created_date")
  updatedDate DateTime? @updatedAt @map("updated_date")
  cart        Cart      @relation(fields: [cartId], references: [id])
  // product ProductListSub

  @@map("cart_item")
}

model Payment {
  id           Int       @id @default(autoincrement())
  ykiho        String
  orderId      String    @unique @map("order_id")
  paymentKey   String    @map("payment_key")
  customerName String    @map("customer_name")
  method       String
  amount       Int
  quantity     Int
  requestedAt  DateTime? @map("requested_at")
  approvedAt   DateTime? @map("approved_at")
  sendType     String    @map("send_type")
  cancel       Boolean
  test         Int?

  // Relation with PaymentItem
  paymentItems PaymentItem[]

  // Relation with PaymentVirtual
  virtual PaymentVirtual?
  refund  PaymentRefund?
}

model PaymentItem {
  id        Int     @id @default(autoincrement())
  paymentId Int
  code      String
  name      String
  fit       Boolean
  quantity  Int
  amount    Float

  // Relation with Payment
  payment Payment @relation(fields: [paymentId], references: [id])

  @@map("payment_item")
}

model PaymentVirtual {
  id            Int      @id @default(autoincrement())
  paymentId     Int      @unique @map("payment_id")
  bankCode      String   @map("bank_code")
  customerName  String   @map("customer_name")
  dueDate       DateTime @map("due_date")
  accountNumber String   @map("account_number")

  // Relation with Payment
  payment Payment @relation(fields: [paymentId], references: [id])

  @@map("payment_virtual")
}

model PaymentRefund {
  id            Int    @id @default(autoincrement())
  paymentId     Int    @unique
  bank          String
  accountNumber String
  holderName    String
  amount        Int
  reason        String

  payment Payment @relation(fields: [paymentId], references: [id])
}

model Em {
